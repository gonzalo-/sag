#!/bin/sh

cd $HOME/.sag || exit 1

  cmds='/usr/bin/nice /bin/sh -c'
cmdseq='/bin/sh bin/t0001'
for sh in bin/t[0-9][0-9][0-9][0-9]
do
    if [ -x $sh ]
    then
        cmdseq="$cmdseq; /bin/sh $sh"
        chmod a-x $sh
    fi
done

export DAYTIME=`date '+%Y %m %d %H %M'`
eval $cmds "\"$cmdseq\"" &
